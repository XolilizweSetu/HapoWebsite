<svg width="800" height="600" viewBox="0 0 800 600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background with African colors -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FF6B35;stop-opacity:0.8" />
      <stop offset="50%" style="stop-color:#F7931E;stop-opacity:0.6" />
      <stop offset="100%" style="stop-color:#FFD23F;stop-opacity:0.4" />
    </linearGradient>
    <linearGradient id="cardGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:0.95" />
      <stop offset="100%" style="stop-color:#f8f9fa;stop-opacity:0.9" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="url(#bgGradient)"/>
  
  <!-- Decorative African pattern -->
  <g opacity="0.1">
    <circle cx="100" cy="100" r="30" fill="#2E8B57"/>
    <circle cx="700" cy="500" r="40" fill="#FF6B35"/>
    <polygon points="50,50 80,20 110,50 80,80" fill="#FFD700"/>
    <polygon points="720,80 750,50 780,80 750,110" fill="#32CD32"/>
  </g>
  
  <!-- Main title -->
  <rect x="50" y="40" width="700" height="80" rx="15" fill="url(#cardGradient)" stroke="#FF6B35" stroke-width="3"/>
  <text x="400" y="70" text-anchor="middle" font-family="Arial, sans-serif" font-size="28" font-weight="bold" fill="#2C3E50">
    🌟 Youth-Led Fintech Innovations in Africa 🌟
  </text>
  <text x="400" y="100" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" fill="#7F8C8D">
    Celebrating Young Innovators Shaping Africa's Financial Future
  </text>
  
  <!-- South Africa Section -->
  <rect x="50" y="150" width="220" height="120" rx="10" fill="url(#cardGradient)" stroke="#0066CC" stroke-width="2"/>
  <text x="160" y="175" text-anchor="middle" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="#0066CC">
    🇿🇦 South Africa
  </text>
  <text x="60" y="195" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#2C3E50">
    Bright On Capital
  </text>
  <text x="60" y="215" font-family="Arial, sans-serif" font-size="12" fill="#34495E">
    Empowering small businesses
  </text>
  <text x="60" y="230" font-family="Arial, sans-serif" font-size="12" fill="#34495E">
    with accessible working capital
  </text>
  <text x="60" y="245" font-family="Arial, sans-serif" font-size="12" fill="#34495E">
    through innovative online
  </text>
  <text x="60" y="260" font-family="Arial, sans-serif" font-size="12" fill="#34495E">
    lending platforms
  </text>
  
  <!-- Egypt Section -->
  <rect x="290" y="150" width="220" height="120" rx="10" fill="url(#cardGradient)" stroke="#DC143C" stroke-width="2"/>
  <text x="400" y="175" text-anchor="middle" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="#DC143C">
    🇪🇬 Egypt
  </text>
  <text x="300" y="195" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#2C3E50">
    Sahl &amp; Khazna
  </text>
  <text x="300" y="215" font-family="Arial, sans-serif" font-size="12" fill="#34495E">
    • Sahl: Simplifying household
  </text>
  <text x="300" y="230" font-family="Arial, sans-serif" font-size="12" fill="#34495E">
    bill payments via mobile
  </text>
  <text x="300" y="245" font-family="Arial, sans-serif" font-size="12" fill="#34495E">
    • Khazna: Mobile banking for
  </text>
  <text x="300" y="260" font-family="Arial, sans-serif" font-size="12" fill="#34495E">
    low &amp; middle-income earners
  </text>
  
  <!-- Morocco Section -->
  <rect x="530" y="150" width="220" height="120" rx="10" fill="url(#cardGradient)" stroke="#228B22" stroke-width="2"/>
  <text x="640" y="175" text-anchor="middle" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="#228B22">
    🇲🇦 Morocco
  </text>
  <text x="540" y="195" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#2C3E50">
    ORA Technologies
  </text>
  <text x="540" y="215" font-family="Arial, sans-serif" font-size="12" fill="#34495E">
    Digital wallets &amp; payment
  </text>
  <text x="540" y="230" font-family="Arial, sans-serif" font-size="12" fill="#34495E">
    services while promoting
  </text>
  <text x="540" y="245" font-family="Arial, sans-serif" font-size="12" fill="#34495E">
    financial literacy among
  </text>
  <text x="540" y="260" font-family="Arial, sans-serif" font-size="12" fill="#34495E">
    Morocco's youth
  </text>
  
  <!-- Key Features Section -->
  <rect x="50" y="300" width="700" height="100" rx="15" fill="url(#cardGradient)" stroke="#FF6B35" stroke-width="3"/>
  <text x="400" y="325" text-anchor="middle" font-family="Arial, sans-serif" font-size="20" font-weight="bold" fill="#FF6B35">
    ✨ Innovation DNA ✨
  </text>
  
  <!-- Mobile First -->
  <circle cx="150" cy="360" r="25" fill="#4CAF50" opacity="0.8"/>
  <text x="150" y="350" text-anchor="middle" font-family="Arial, sans-serif" font-size="20">📱</text>
  <text x="150" y="385" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#2C3E50">
    Mobile-First
  </text>
  
  <!-- Inclusive -->
  <circle cx="400" cy="360" r="25" fill="#2196F3" opacity="0.8"/>
  <text x="400" y="350" text-anchor="middle" font-family="Arial, sans-serif" font-size="20">🤝</text>
  <text x="400" y="385" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#2C3E50">
    Inclusive Design
  </text>
  
  <!-- Government Partnership -->
  <circle cx="650" cy="360" r="25" fill="#9C27B0" opacity="0.8"/>
  <text x="650" y="350" text-anchor="middle" font-family="Arial, sans-serif" font-size="20">🏛️</text>
  <text x="650" y="385" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#2C3E50">
    Gov Partnership
  </text>
  
  <!-- Investment Highlight -->
  <rect x="50" y="430" width="700" height="80" rx="15" fill="#E8F5E8" stroke="#4CAF50" stroke-width="3"/>
  <text x="400" y="455" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#2E7D32">
    💰 $10+ Billion in Fintech Investments 💰
  </text>
  <text x="400" y="480" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#388E3C">
    Youth-led ventures capturing global investor attention
  </text>
  <text x="400" y="500" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#388E3C">
    across the African continent
  </text>
  
  <!-- Youth Month Celebration Banner -->
  <rect x="50" y="530" width="700" height="50" rx="25" fill="#FFD700" stroke="#FF6B35" stroke-width="2"/>
  <text x="400" y="555" text-anchor="middle" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="#8B4513">
    🎉 Celebrating Youth Month - Honoring Africa's Young Innovators 🎉
  </text>
</svg>